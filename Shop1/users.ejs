<html>
  <head>
    <title>Kunden</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
  </head>
  <body>
    <h1>Kunden</h1>
    <ul>
      <% for(var i = 0; i<kunden.length;i++){
          %>
          <h2>Kunde <%= i+1 %> </h2>
          <li><%= kunden[i].benutzer %></li>
          <li><%= kunden[i].name %></li>
          <li><%= kunden[i].vorname %></li>
          <li><%= kunden[i].kundenID %></li>
          <%
          }
          %>
    </ul>
  </body>
</html>
